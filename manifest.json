{
    "manifest_version": 2,
    "name": "Session Saver",
    "version": "1.0",

    "background": {
        "persistent": false,
        "scripts": ["TabBackGround.js"]
    },

    "icons": {
        "16": "session-16.png",
        "48": "session-48.png",
        "128": "session-128.png"
    },

    "browser_action": {
        "default_icon": "session-16.png",
        "default_popup": "popup.html"
    },

    "options_page": "options.html",

    "permissions": [
          "tabs",
          "storage"
    ],

    "commands":
    {
        "save-and-close" : 
        {
            "suggested_key": {
                "default": "Ctrl+Shift+U",
                "windows": "Ctrl+Shift+U",
                "mac": "Command+Shift+U",
                "chromeos": "Ctrl+Shift+U",
                "linux": "Ctrl+Shift+U"
            },
            "description" : "Saves current window and closes it."
        }
    }

}