{
    "manifest_version": 2,
    "name": "Session Saver",
    "version": "0.1",

    "content_scripts": [
        {
            "matches": [
              "<all_urls>"
            ],
            "js": ["content.js", "jquery-3.2.1.min.js"]
        }
    ], 

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "permissions": [
          "tabs",
          "storage"
    ]


}